{
    "id": "rosbot",
    "name": {
        "en": "Rosbot",
        "zh-cn": "Rosbot"
    },
    "description": {
        "en": "A robot controller based on arduino 328p",
        "zh-cn": "基于arduino 328p的机器人主板"
    },
    "color1": "#DE5277",
    "color2": "#AA3F5B",
    "color3": "#AA3F5B",
    "version": "2.3.0",
    "io": [
        "serial"
    ],
    "listdevice": {
        "pid": "0x7523",
        "vid": "0x1a86"
    },
    "masterExt": "rosbot",
    "type": "arduino",
    "fwtype": "arduino",
    "protocol": "command",
    "firmware": "kittenbot.hex",
    "arduino": {
        "core": "arduino:avr",
        "board": "uno",
        "libs": [
            "TimerFreeTone.zip",
            "Adafruit_GFX.zip",
            "Adafruit_LED_Backpack.zip",
            "Adafruit_NeoPixel.zip",
            "AccelStepper.zip",
            "DHT11.zip",
            "FlexiTimer2.zip",
            "KittenBot.zip",
            "kittenbotEE.zip",
            "KittenbotIoT.zip",
            "VarSpeedServo.zip",
            "PS2X_lib.zip"
        ],
        "objects": {
            "kb": {
                "import": "#include \"KittenBot.h\"",
                "object": "KittenBot kb;",
                "setup": "kb.init()"
            },
            "ds18b20": {
                "import": "#include <OneWire.h>",
                "object": "OneWire oneWire(2);",
                "setup": "sensors_2.getAddress(insideThermometer, 0);"
            },
            "softPWM": {
                "import": "#include <SoftPWM.h>",
                "setup": "SoftPWMBegin();"
            },
            "simpleTimer": {
                "import": "#include <SimpleTimer.h>",
                "object": "SimpleTimer timer;",
                "setup": "timer.setInterval(1000L, Simple_timer_1);"
            }
        }
    },
    "subextensions": [
        "sugar.json5"
    ],
    "errorhelper": {
        "arduino_core": {
            "msg": "missing avr core",
            "event": [
                "Install Avr Core",
                "arduino-install-pkg",
                "arduino:avr"
            ]
        }
    },
    "kittenext": true,
    "target": "uno",
    "helpLink": "https://www.yuque.com/kittenbot/hardwares/rosbot-kittenblock",
    "shopLink": "https://item.taobao.com/item.htm?spm=a1z10.1-c-s.w5003-21580290920.14.2102169fuqup0t&id=551011963085",
    "image": "Rosbot.png",
    "weights": 499
}